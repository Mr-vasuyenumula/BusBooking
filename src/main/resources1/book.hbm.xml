<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.bus.pojo">
<class name="Bus" table="userbus">
  <id name="bid" column="bid">
  <generator class="increment"></generator>
  
  </id>
  <property name="bfrom" column="bus_from" length="20"></property>
  <property name="bto" column="bus_to" length="20"></property>
  <property name="busno" column="busno" length="20"></property>
   <property name="busname" column="busname" length="20"></property>
    <property name="type" column="type" length="20"></property>
   <property name="datevalue" column="datevalue" type="timestamp" length="20"></property>
 


</class>




</hibernate-mapping>